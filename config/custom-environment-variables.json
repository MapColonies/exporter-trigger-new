{
  "domain": "JOB_DOMAIN",
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "telemetry": {
    "serviceName": "TELEMETRY_SERVICE_NAME",
    "hostname": "TELEMETRY_HOST_NAME",
    "version": "TELEMETRY_SERVICE_VERSION",
    "logger": {
      "level": "LOG_LEVEL",
      "prettyPrint": {
        "__name": "LOG_PRETTY_PRINT_ENABLED",
        "__format": "boolean"
      }
    },
    "tracing": {
      "enabled": "TELEMETRY_TRACING_ENABLED",
      "url": "TELEMETRY_TRACING_URL"
    },
    "metrics": {
      "enabled": "TELEMETRY_METRICS_ENABLED",
      "url": "TELEMETRY_METRICS_URL",
      "interval": "TELEMETRY_METRICS_INTERVAL"
    }
  },
  "server": {
    "port": { "__name": "SERVER_PORT", "__format": "number" },
    "request": {
      "payload": {
        "limit": "REQUEST_PAYLOAD_LIMIT"
      }
    },
    "response": {
      "compression": {
        "enabled": {
          "__name": "RESPONSE_COMPRESSION_ENABLED",
          "__format": "boolean"
        }
      }
    }
  },
  "jobDefinitions": {
    "jobs": {
      "export": {
        "type": "EXPORT_JOB_TYPE"
      }
    },
    "tasks": {
      "init": {
        "type": "INIT_TASK_TYPE"
      }
    }
  },
  "cleanupExpirationDays": {
    "__name": "CLEANUP_EXPIRATION_DAYS",
    "__format": "number"
  },
  "tilesProvider": "TILES_PROVIDER",
  "gpkgsLocation": "GPKGS_LOCATION",
  "externalClientsConfig": {
    "clientsUrls": {
      "jobManager": {
        "url": "JOB_MANAGER_URL"
      },
      "rasterCatalogManager": {
        "url": "RASTER_CATALOG_MANAGER_URL"
      }
    },
    "httpRetry": {
      "attempts": {
        "__name": "HTTP_RETRY_ATTEMPTS",
        "__format": "number"
      },
      "delay": "HTTP_RETRY_DELAY",
      "shouldResetTimeout": {
        "__name": "HTTP_RETRY_SHOULD_RESET_TIMEOUT",
        "__format": "boolean"
      }
    },
    "disableHttpClientLogs": {
      "__name": "DISABLE_HTTP_CLIENT_LOGS",
      "__format": "boolean"
    }
  },
  "roiBufferMeter": {
    "__name": "ROI_BUFFER_METER",
    "__format": "number"
  },
  "minContainedPercentage": {
    "__name": "MIN_CONTAINED_PERCENTAGE",
    "__format": "number"
  }
}
